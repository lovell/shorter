{
  "name": "shorter",
  "version": "0.2.2",
  "author": "Lovell Fuller <npm@lovell.info>",
  "description": "Node.js module to very quickly (de)compress short strings. Uses the SIMD-powered entropy encoding features of shoco.",
  "scripts": {
    "test": "semistandard && cc && node test/unit"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/lovell/shorter"
  },
  "keywords": [
    "string",
    "compression",
    "decompression",
    "entropy",
    "encoding",
    "shoco",
    "smaz",
    "short",
    "simd"
  ],
  "dependencies": {
    "nan": "^2.11.0"
  },
  "devDependencies": {
    "cc": "^1.0.2",
    "semistandard": "^12.0.1"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4"
  },
  "semistandard": {
    "ignore": "src/"
  },
  "cc": {
    "linelength": "90",
    "ignore": [
      "src/shoco/**"
    ]
  }
}
